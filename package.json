{
	"name": "@igor.dvlpr/chars-in-string",
	"version": "2.0.0",
	"description": "🪐 Provides ways of testing whether an array of chars is present inside a given String. ☄",
	"type": "module",
	"engines": {
		"node": ">=12.17"
	},
	"sideEffects": false,
	"main": "main",
	"exports": {
		".": {
			"require": "./dist/compat/main.cjs",
			"import": "./dist/module/main.js"
		}
	},
	"module": "./dist/module/main.js",
	"types": "./dist/main",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf ./dist/*",
		"build:cjs": "cross-env BABEL_ENV=cjs babel src -d dist/compat --out-file-extension .cjs",
		"build:esm": "cross-env BABEL_ENV=esm babel src -d dist/module",
		"build:dts": "tsc",
		"build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:dts",
		"test": "mocha",
		"pub": "npm run build && npm test && npm publish --access public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/igorskyflyer/npm-chars-in-string.git"
	},
	"keywords": [
		"chars",
		"string",
		"array",
		"check",
		"test",
		"contains",
		"igor.dvlpr"
	],
	"author": "Igor Dimitrijević",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/igorskyflyer/npm-chars-in-string/issues"
	},
	"homepage": "https://github.com/igorskyflyer/npm-chars-in-string#readme",
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/plugin-transform-modules-commonjs": "^7.14.5",
		"@babel/preset-env": "^7.14.9",
		"@types/chai": "^4.2.21",
		"@types/mocha": "^9.0.0",
		"@types/node": "^12.17.0",
		"babel-plugin-add-import-extension": "^1.6.0",
		"chai": "^4.3.4",
		"cross-env": "^7.0.3",
		"mocha": "^9.0.2",
		"rimraf": "^3.0.2",
		"typescript": "^4.3.5"
	}
}
