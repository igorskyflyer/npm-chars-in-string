{
    "name": "@igorskyflyer/chars-in-string",
    "version": "3.0.0",
    "description": "🪐 Determines whether an array of chars is present inside a given String. ☄",
    "sideEffects": false,
    "engines": {
        "node": ">=22.0"
    },
    "type": "module",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "test": "vitest --run",
        "pub": "pnpm test && git push origin main --tags"
    },
    "keywords": [
        "string-search",
        "string-utils",
        "string-matching",
        "substring",
        "text-search",
        "find-in-string",
        "character-search",
        "substring-search",
        "case-sensitive-search",
        "case-insensitive-search",
        "string-position",
        "starts-with",
        "ends-with",
        "typescript",
        "ts",
        "igor dimitrijević",
        "igorskyflyer"
    ],
    "license": "MIT",
    "author": "Igor Dimitrijević <support@igorskyflyer.me> (https://github.com/igorskyflyer)",
    "funding": "https://ko-fi.com/igorskyflyer",
    "homepage": "https://github.com/igorskyflyer/npm-chars-in-string",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/igorskyflyer/npm-chars-in-string.git"
    },
    "bugs": {
        "url": "https://github.com/igorskyflyer/npm-chars-in-string/issues"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.2",
        "@igorskyflyer/biome-config": "^2.1.0",
        "@types/node": "^22.18.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "pnpm@10.15.0"
